<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:table="urn:import:org.openelis.ui.widget.table"
  xmlns:shl="urn:import:org.openelis.ui.widget">

  <g:LayoutPanel ui:field="outer">
    <g:layer left="0" right="16px">
      <g:ScrollPanel ui:field="scrollView">
        <g:LayoutPanel ui:field="inner">
          <g:layer top="0px" height="25px">
            <table:Header ui:field="header" visible="false" />
          </g:layer>
          <g:layer top="25px" bottom="0px">
            <g:FocusPanel ui:field="fp" width="100%" height="100%">
              <g:FlexTable ui:field="flexTable" />
            </g:FocusPanel>
          </g:layer>
        </g:LayoutPanel>
      </g:ScrollPanel>
    </g:layer>
    <g:layer right="0px" width="16px">
      <shl:VerticalScrollbar ui:field="vertScrollBar" visible="false" />
    </g:layer>
  </g:LayoutPanel>

</ui:UiBinder>