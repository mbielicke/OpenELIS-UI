<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:g="urn:import:com.google.gwt.user.client.ui"
             ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat'
             ui:generateLocales="default">

  <ui:with field="res" type="org.openelis.ui.resources.UIResources"/>
  <ui:with field="msg" type="org.openelis.ui.messages.UIMessages"/>

  <g:Grid ui:field="table" styleName="{res.calendarui.CalendarTable}" cellSpacing="2">
    <g:row styleName="{res.calendarui.DayBar}">
      <g:cell>
        <ui:text from="{msg.cal_day0}"/>
      </g:cell>
      <g:cell>
        <ui:text from="{msg.cal_day1}"/>
      </g:cell>
      <g:cell>
        <ui:text from="{msg.cal_day2}"/>
      </g:cell>
      <g:cell>
        <ui:text from="{msg.cal_day3}"/>
      </g:cell>
      <g:cell>
        <ui:text from="{msg.cal_day4}"/>
      </g:cell>
      <g:cell>
        <ui:text from="{msg.cal_day5}"/>
      </g:cell>
      <g:cell>
        <ui:text from="{msg.cal_day6}"/>
      </g:cell>
    </g:row>
    <g:row>
      <g:cell/>
      <g:cell/>
      <g:cell/>
      <g:cell/>
      <g:cell/>
      <g:cell/>
      <g:cell/>
    </g:row>
    <g:row>
      <g:cell/>
      <g:cell/>
      <g:cell/>
      <g:cell/>
      <g:cell/>
      <g:cell/>
      <g:cell/>
    </g:row>
    <g:row>
      <g:cell/>
      <g:cell/>
      <g:cell/>
      <g:cell/>
      <g:cell/>
      <g:cell/>
      <g:cell/>
    </g:row>
    <g:row>
      <g:cell/>
      <g:cell/>
      <g:cell/>
      <g:cell/>
      <g:cell/>
      <g:cell/>
      <g:cell/>
    </g:row>
    <g:row>
      <g:cell/>
      <g:cell/>
      <g:cell/>
      <g:cell/>
      <g:cell/>
      <g:cell/>
      <g:cell/>
    </g:row>
    <g:row>
      <g:cell/>
      <g:cell/>
      <g:cell/>
      <g:cell/>
      <g:cell/>
      <g:cell/>
      <g:cell/>
    </g:row>  
  </g:Grid>

</ui:UiBinder>