<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:w="urn:import:org.openelis.ui.widget">

  <ui:with type="org.openelis.ui.resources.UIResources" field="res" />

  <g:LayoutPanel ui:field="outer">
    <g:layer top="0px" height="25px">
      <g:LayoutPanel ui:field="top">
        <g:layer top="0px" bottom="0px" left="0px" right="16px">
          <w:Window.Caption ui:field="cap" width="100%" />
        </g:layer>
        <g:layer right="0px" width="16px" top="0px" bottom="0px">
          <g:FocusPanel ui:field="close" width="16px" />
        </g:layer>
      </g:LayoutPanel>
    </g:layer>
    <g:layer top="25px" bottom="0px">
      <g:LayoutPanel ui:field="body" >
        <g:layer top="0px" bottom="40px">
          <g:LayoutPanel>
            <g:layer left="0px" width="32px">
              <g:AbsolutePanel ui:field="icon"/>
            </g:layer>
            <g:layer left="32px" right="0px">
              <g:HTML ui:field="text" width="100%" height="100%"/>
            </g:layer>
          </g:LayoutPanel>
        </g:layer>
        <g:layer bottom="0px" height="40px">
          <g:HorizontalPanel ui:field="buttonPanel"/>
        </g:layer>
      </g:LayoutPanel>
    </g:layer>
  </g:LayoutPanel>

</ui:UiBinder>
