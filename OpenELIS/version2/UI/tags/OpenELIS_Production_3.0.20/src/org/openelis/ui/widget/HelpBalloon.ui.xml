<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:g="urn:import:com.google.gwt.user.client.ui"
             xmlns:w="urn:import:org.openelis.ui.widget">
             
    <ui:with field="res" type="org.openelis.ui.resources.UIResources"/>

    <g:AbsolutePanel ui:field="tip" styleName="{res.help.ToolTip}">
      <g:AbsolutePanel ui:field="nub" styleName="{res.help.Nub}"/>
      <g:AbsolutePanel ui:field="content" styleName="{res.help.Content}"/>
    </g:AbsolutePanel>
        
</ui:UiBinder>