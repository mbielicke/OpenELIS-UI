<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:g="urn:import:com.google.gwt.user.client.ui"
             xmlns:w="urn:import:org.openelis.ui.widget">
             
    <ui:with field="res" type="org.openelis.ui.resources.UIResources"/>


    <g:LayoutPanel ui:field="display">
      <g:layer left="0px" right="18px">
        <w:TextBase ui:field="textbox" width="100%"/>
      </g:layer>
      <g:layer right="0" width="18px">
        <g:FocusPanel ui:field="button"/>
      </g:layer>
    </g:LayoutPanel>
    
</ui:UiBinder>