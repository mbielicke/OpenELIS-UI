<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:g="urn:import:com.google.gwt.user.client.ui"
             xmlns:w="urn:import:org.openelis.ui.widget">
             
    <ui:with field="res" type="org.openelis.ui.resources.UIResources"/>

    <g:LayoutPanel ui:field="display">
      <g:layer left="0px" right="16px">
        <w:TextBase ui:field="textbox" width="100%"/>
      </g:layer>
      <g:layer right="0" width="16px">
       <w:Button ui:field="button" CSS="{res.lookupButton}"/>
      </g:layer>
    </g:LayoutPanel>
    
</ui:UiBinder>